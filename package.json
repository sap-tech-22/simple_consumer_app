{
  "name": "simple_consumer_app",
  "version": "0.0.1",
  "description": "Simple consumer application from HDI container",
  "dependencies": {
    "@sap/cds": "^5",
    "@sap/audit-logging": "^3",
    "@sap/hana-client": "^2",

    "debug": "^4.3",
    "express": "^4"
  },
  "devDependencies": {
    "sqlite3": "5.0.0"
  },
  "scripts": {
    "start": "npx cds run"
  },
  "cds": {
    "requires": {
      "db": {
        "[production]": {
          "kind": "hana"
        },
        "[development]": {
          "kind": "sql"
        }
      }
    }
    ,
    "hana": {
      "deploy-format": "hdbtable"
    }
  }
}
